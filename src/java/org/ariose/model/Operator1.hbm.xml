<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.ariose.model.Operator" table="operator1">

        <id name="operator_id" column="operator_id" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <property name="operator_name" column="operator_name" not-null="false"/>
   </class>
</hibernate-mapping>