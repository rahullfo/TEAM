<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Document   : AirtelGatewayDetails.hbm.xml
    Created on : February 19, 2009, 1:57 PM
    Author     : Sukhdeep
    Description:
        Purpose of the document is to map AirtelGatewayDetailsBean class with the airtel_gateway_details table in the DataBase.
-->
<hibernate-mapping>
    <class name="org.ariose.model.EmailUserBean" table="emailuser">

        <id name="id" column="id" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <property name="name" column="name" not-null="false"/>
        <property name="mobile" column="mobile" not-null="false"/>
        <property name="email" column="email" not-null="false"/>
        <property name="operatorName" column="operatorname" not-null="false"/>
        <property name="operatorId" column="operatorid" not-null="true"/>
        <property name="circleName" column="circlename" not-null="false"/>
        <property name="circleId" column="circleid" not-null="false"/>
        <property name="packName" column="packname" not-null="false"/>
        <property name="packId" column="packid" not-null="false"/>
        <property name="subscriptionId" column="subscriptionid" not-null="false"/>
    </class>
</hibernate-mapping>