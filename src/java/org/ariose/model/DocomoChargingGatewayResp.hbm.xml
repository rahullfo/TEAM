<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.ariose.model.DocomoChargingGatewayResp" table="docomo_charging_gateway_resp">

		<id name="id" column="id" unsaved-value="0">
			<generator class="increment" />
		</id>
        <property name="subscriberNumber" column="subscriber_number" not-null="false"/>
        <property name="packId" column="pack_id" not-null="false"/>
        <property name="packName" column="pack_name" not-null="false"/>
        <property name="subscriptionId" column="subscription_id" not-null="false"/>
        <property name="subscriptionStatus" column="subscription_status" not-null="false"/>
        <property name="subscriptionStatusMeaning" column="subscription_status_meaning" not-null="false"/>
        <property name="amount" column="amount" not-null="false"/>
        <property name="minutes" column="minutes" not-null="false"/>
        <property name="subscriptionFailId" column="subscription_fail_id" not-null="false"/>
        <property name="validDate" column="valid_date" not-null="false"/>
        <property name="status" column="status" not-null="false"/>
        <property name="statusMeaning" column="status_meaning" not-null="false"/>
        <property name="logtime" column="logtime" not-null="false"/>
        <property name="updateTime" column="update_time" not-null="false"/>
	</class>
</hibernate-mapping>
