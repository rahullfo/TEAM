<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Document   : RelianceBillingConfig.hbm.xml
    Created on : August 09, 2010
    Author     : Manu
    Description:
        Purpose of the document is to map RelianceBillingConfig class with the reliancebillingconfig table in the DataBase.
-->
<hibernate-mapping>
    <class name="org.ariose.model.RelianceBillingConfig" table="reliance_billing_config">

        <id name="id" column="id" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <property name="billingkey"/>
        <property name="value"/>
    </class>
</hibernate-mapping>