<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Document   : Circle.hbm.xml
    Created on : February 27, 2009, 2:45 PM
    Author     : Sukhdeep
    Description:
        Purpose of the document is to map CircleBean class with the circle in the DataBase.
-->

<hibernate-mapping>
    <class name="org.ariose.model.LanguageBean" table="language">

        <id name="languageId" column="l_id" unsaved-value="0">
            <generator class="increment"/>
        </id>
        <property name="languageName" column="l_name" not-null="true"/>
   </class>
</hibernate-mapping>
